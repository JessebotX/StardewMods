{
	"Format": "1.4",
	"ConfigSchema": {
		"PlainGreenhouse": {
			"AllowValues": "disabled, enabled",
		},
	},
	"Changes": [
		
		//load greenhouse
		{
			"Action": "Load",
			"Target": "Maps/Greenhouse",
			"FromFile": "assets/Greenhouse.tbin",
			"When": {
				"PlainGreenhouse": "disabled",
			},
		},
		
		//load plain greenhouse
		{
			"Action": "Load",
			"Target": "Maps/Greenhouse",
			"FromFile": "assets/Greenhouse_plain.tbin",
			"When": {
				"PlainGreenhouse": "enabled",
			},
		},
	]
}